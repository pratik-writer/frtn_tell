    <script type="text/javascript">
        function MaintainDatePicker() {
            $('.date').datepicker({
                todayBtn: "linked",
                keyboardNavigation: false,
                forceParse: false,
                calendarWeeks: true,
                autoclose: true
            }).on('changeDate', function (ev) {
                $(this).trigger('change');
                $(this).trigger('blur');
            });
            $('.dateTime').datetimepicker({
                language: 'en',
                pick12HourFormat: true
            });
        }
        $(function () {
            MaintainDatePicker();
            $.validator.setDefaults({ ignore: ":hidden:not(.chosen-select)" })

            // full overloay on all AJAX calls
            $(document).ajaxStart(function () { tId = setTimeout(function () { if ($.active) { blockUI(); } }, 50); });
            $(document).ajaxStop(function () { clearTimeout(tId); unblockUI(); });

        });
        function goToByScroll(id) {
            // Remove "link" from the ID
            id = id.replace("link", "");
            // Scroll
            $('html,body').animate({
                scrollTop: $("#" + id).offset().top
            },
                'slow');
        }

        jQuery.validator.setDefaults({
            focusInvalid: false,
            invalidHandler: function (event, validator) {
                if (!validator.numberOfInvalids())
                    return;
                event.preventDefault();
                $('html, body').animate({
                    scrollTop: $(validator.errorList[0].element).offset().top - 284
                }, 1000);

                toastr.error("Please fill in the highlighted fields");
            },
            errorElement: 'span',
            errorPlacement: function (error, element) {
                if (element.attr("type") == "radio" || element.attr("type") == "checkbox")
                    error.insertAfter($(element).parent().parent()[0]);
                else
                    error.insertAfter(element);
            }
        });
        var current = location.pathname;
        $(function () {
            $('nav li a').each(function () {
                var $this = $(this);
                if ($this.attr('href') && $this.attr('href').indexOf(current) !== -1) {
                    $this.parent("li").addClass("active");
                    parent = $this.closest("ul").parent("li").find('a:first').trigger("click")
                }
            })
        })

    </script>
    
